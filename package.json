{
  "name": "nestjs-typeorm-custom-repository",
  "version": "1.0.3",
  "description": "Typeorm custom repository module for Nest framework (node.js)",
  "author": "duynk98vn",
  "license": "MIT",
  "url": "https://github.com/duynk98vn/nestjs-typeorm-custom-repository#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"{lib,tests}/**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test": "jest --config ./tests/jest.config.json --detectOpenHandles",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0",
    "@nestjs/typeorm": ">= 8.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/common": "^9.4.2",
    "@nestjs/testing": "^9.4.2",
    "@nestjs/typeorm": "^9.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "pg": "^8.11.0",
    "prettier": "^2.8.8",
    "reflect-metadata": "^0.1.13",
    "release-it": "^15.10.5",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.0",
    "typeorm": "^0.3.16",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  }
}
